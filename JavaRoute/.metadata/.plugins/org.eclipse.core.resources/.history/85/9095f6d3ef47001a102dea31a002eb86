package File;
import java.io.*;
public class CharFile {
	public static void main(String[] args) {
		File f = new File("F:\\代码文件\\代码库\\java  路线\\j2ee\\src\\File\\lol.txt");
		String str=nmull;
		try(FileReader fr = new FileReader(f)){
			char all [] = new char[(int) f.length()];
			fr.read(all);
			for (char b : all) {
				System.out.println(b);
			}
			fr.write();
		}
		catch(IOException e) {
			e.printStackTrace();
		}
	}
}
