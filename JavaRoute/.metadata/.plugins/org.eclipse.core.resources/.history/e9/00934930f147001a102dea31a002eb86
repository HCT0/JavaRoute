package File;
import java.io.*;
public class CharFilePractice {
	public static void main(String[] args) {
		File f = new File("F:\\代码文件\\代码库\\java  路线\\j2ee\\src\\File\\CharFilePractice");
		try(FileReader fr = new  FileReader(f)){
			char all[] = new char[(int) f.length() ];
			for(char b : fr)
				System.out.print(b);
		}
		catch(IOException e) {
			e.printStackTrace();
		}
		
	}
}
