package j2ee;

public class Hero {
	String name;//英雄的名字
	int moveSpeend;//移动速度
	float hp;//英雄的血量
	float armor;//英雄的护甲
	int dietNumber;//英雄四万的次数
	
	public static void main(String[] args) {
		Hero timo = new Hero();
		timo.armor = 14f;  
		timo.hp = 383f;
		timo.moveSpeend = 330;
		timo.name = "提莫";
		
		Hero gailun = new Hero();
		gailun.armor = 27.536f;
		gailun.name = "盖伦";
		gailun.hp = 616.28f;
		gailun.moveSpeend = 350;
	}
} 
