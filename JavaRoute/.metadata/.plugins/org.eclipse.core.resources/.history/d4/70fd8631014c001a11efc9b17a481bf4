package SetFramework;
import LOL.*;
import java.util.*;
public class ContainerClass {
	public static void main(String[] args) {
		ArrayList heros = new ArrayList();
		heros.add(new Hero("gailun"));
		System.out.println(heros.size());
		
		heros.add(new Hero("temo"));
		System.out.println(heros.size());
	
	}
}
